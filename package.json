{
  "name": "vuetable-3",
  "version": "2.0.0-beta.4",
  "description": "Datatable component for Vue 3.x",
  "main": "dist/vuetable-3.js",
  "license": "MIT",
  "author": "Rati Wannapanop <rati.wannapanop@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janwehner/vuetable-3.git"
  },
  "private": false,
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "browserify": {
    "transform": [
      "babelify",
      [
        "vueify",
        {
          "_flags": {
            "debug": true
          }
        }
      ]
    ]
  },
  "scripts": {
    "serve": "webpack serve --config build/webpack.dev.conf.js --hot --progress",
    "make": "cross-env MINIFY=true webpack --progress --config build/webpack.prod.conf.js",
    "build": "node build/build.js && yarn run bundle",
    "bundle:min": "cross-env MINIFY=true webpack --progress --config build/webpack.build.config.js",
    "bundle:full": "cross-env MINIFY=false webpack --progress --config build/webpack.build.config.js",
    "bundle": "yarn run bundle:min && yarn run bundle:full",
    "test": "yarn jest"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "terser-webpack-plugin": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/test-utils": "^2.0.0-rc.6",
    "add": "^2.0.6",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babelify": "^10.0.0",
    "chalk": "^4.1.1",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "8.1.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^5.2.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "5.0.0-alpha.8",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "2.0.0-beta.2",
    "html-webpack-plugin": "^5.3.1",
    "http-proxy-middleware": "^1.2.1-alpha.1",
    "inject-loader": "^4.0.1",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "lolex": "^6.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "mocha": "^8.3.2",
    "moxios": "^0.4.0",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "ora": "5.4.0",
    "postcss": "^8.2.12",
    "postcss-loader": "^5.2.0",
    "promise-polyfill": "^8.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "stats-webpack-plugin": "0.7.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue": "3.0.11",
    "vue-html-loader": "1.2.4",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-loader": "^16.2.0",
    "vue-style-loader": "4.1.3",
    "webpack": "^5.35.0",
    "webpack-bundle-analyzer": "4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.7.3",
    "yarn": "^1.22.10"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
